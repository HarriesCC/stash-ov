name: Disney+双字幕
desc: 显示第二英文字幕

http:
  mitm:
    - *.media.dssott.com
  script:
    - match: https:\/\/.+media.dssott.com\/ps01\/disney\/.+(\.vtt|-all-.+\.m3u8.*)
      name: disney-plus-sualsub
      type: response
      require-body: true
      timeout: 5
script-providers:
  disney-plus-sualsub:
    url: https://raw.githubusercontent.com/Neurogram-R/Surge/master/DisneyPlus-Dualsub.js
    interval: 86400
